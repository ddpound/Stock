<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.AreaChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.BuySellController">
   <children>
      <Label layoutX="14.0" layoutY="24.0" text="주식 팔고 사는창" />
      <Button layoutX="311.0" layoutY="444.0" mnemonicParsing="false" onAction="#btnBuy" text="BUY" />
      <Button layoutX="395.0" layoutY="444.0" mnemonicParsing="false" onAction="#btnSell" text="SELL" />
      <ListView fx:id="stockList" layoutX="14.0" layoutY="310.0" prefHeight="123.0" prefWidth="192.0" />
      <AreaChart fx:id="stockArea" layoutX="11.0" layoutY="66.0" prefHeight="228.0" prefWidth="466.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </AreaChart>
   </children>
</AnchorPane>
